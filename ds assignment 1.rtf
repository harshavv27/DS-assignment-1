Question 4 (A) \'96 Reverse a Character Array\
\
#include <iostream>\
using namespace std;\
\
void reverseArray(char arr[], int n) \{\
    char temp;\
    for (int i = 0; i < n / 2; i++) \{\
        temp = arr[i];\
        arr[i] = arr[n - i - 1];\
        arr[n - i - 1] = temp;\
    \}\
\}\
\
int main() \{\
    char arr[100];\
    int n;\
\
    cout << "Enter the total number of characters: ";\
    cin >> n;\
\
    cout << "Enter the characters: ";\
    for (int i = 0; i < n; i++) \{\
        cin >> arr[i];\
    \}\
\
    reverseArray(arr, n);\
\
    cout << "Reversed array: ";\
    for (int i = 0; i < n; i++) \{\
        cout << arr[i];\
    \}\
    cout << endl;\
\
    return 0;\
\}\
