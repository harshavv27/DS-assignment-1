Question 2 \'96 Remove Duplicates from an Array\
\
#include <iostream>\
using namespace std;\
\
void removeDuplicates(int arr[], int &n) \{\
    for (int i = 0; i < n; i++) \{\
        for (int j = i + 1; j < n; j++) \{\
            if (arr[i] == arr[j]) \{\
                // Shift elements left to remove duplicate\
                for (int k = j; k < n - 1; k++) \{\
                    arr[k] = arr[k + 1];\
                \}\
                n--;   // Reduce size of array\
                j--;   // Recheck current index after shifting\
            \}\
        \}\
    \}\
\}\
\
int main() \{\
    int arr[100], n;\
\
    cout << "Enter number of elements: ";\
    cin >> n;\
\
    cout << "Enter " << n << " elements: ";\
    for (int i = 0; i < n; i++) \{\
        cin >> arr[i];\
    \}\
\
    removeDuplicates(arr, n);\
\
    cout << "Array after removing duplicates: ";\
    for (int i = 0; i < n; i++) \{\
        cout << arr[i] << " ";\
    \}\
    cout << endl;\
\
    return 0;\
\}\
